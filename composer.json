{
  "name": "kz370/scollio-logger",
  "description": "Scollio Logger: modern, database-backed logging with a dashboard",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Kz370",
      "email": "kz.dev370@gmail.com"
    }
  ],
  "keywords": ["laravel", "logging", "logger", "psr-3", "dashboard"],
  "support": {
    "issues": "https://github.com/kz370/scollio-logger/issues",
    "source": "https://github.com/kz370/scollio-logger"
  },
  "require": {
    "php": ">=8.0",
    "illuminate/support": "^8.0|^9.0|^10.0|^11.0",
    "illuminate/database": "^8.0|^9.0|^10.0|^11.0",
    "illuminate/routing": "^8.0|^9.0|^10.0|^11.0",
    "illuminate/view": "^8.0|^9.0|^10.0|^11.0",
    "psr/log": "^3.0"
  },
  "autoload": {
    "psr-4": {
      "Kz370\\ScollioLogger\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Kz370\\ScollioLogger\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Kz370\\ScollioLogger\\ScollioLoggerServiceProvider"
      ],
      "aliases": {
        "ScollioLogger": "Kz370\\ScollioLogger\\Facades\\ScollioLogger"
      }
    }
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}
